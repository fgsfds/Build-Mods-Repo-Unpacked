action AMARIODEAD
move MARIOSHRUNK

state mariocode
ifmove 0
{
	move STOPPED
	geta[].xrepeat startx
	geta[].yrepeat starty
	set countvar startx
	shiftr countvar 1
	set countvarb starty
	shiftr countvarb 1
	set FEMFALLDMG 256
	cstator 256
}


ifaction AMARIODEAD
{
	ifcount 40
	{
		geta[].z z
		add z FEMFALLDMG
		add FEMFALLDMG 128
		seta[].z z
	} 
	else
	ifcount 39 cstator 8
	else
	ifcount 16 nullop 
	else
	{
		geta[].z z
		sub z 1280
		seta[].z z
	}
	ifcount 80 killit
	break
}
fall

ifmove MARIOSHRUNK
{
	ifcount 30 
	{
		seta[].xrepeat countvar
		seta[].yrepeat countvarb
		strength 0
	}
	else
	{
		add countvarc 1
		ifg countvarc 3
		{
			set countvarc 0
			ife sprite[].xrepeat startx
			{
				seta[].xrepeat countvar
				seta[].yrepeat countvarb
			}
			else
			{
				seta[].xrepeat startx
				seta[].yrepeat starty
			}
		}
		break
	}
}

ifhitweapon
{
	ifmove STOPPED
	{
		move MARIOSHRUNK
		sound MARIODAMAGE
		strength 0
		break
	}
	ifmove MARIOSHRUNK
	{
		sound MARIODIES
		action AMARIODEAD
		resetcount
	}
}

ends


