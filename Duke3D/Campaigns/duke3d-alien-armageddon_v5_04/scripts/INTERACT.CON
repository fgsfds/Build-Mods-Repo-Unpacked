  {
	ifspawnedby APLAYER sizeat 32 32
    action ANULLACTION
    cstator 256
	
	set temp tiledata[sprite[].picnum].xsize
	ifg tiledata[sprite[].picnum].ysize temp set temp tiledata[sprite[].picnum].xsize
	mul temp sprite[].xrepeat
	ifg temp 1560 cstator 1 else
	{
		geta[].cstat tempb
		ifvarand tempb 1 xorvar tempb 1
		seta[].cstat tempb
	}
    ifactor ROBOTMOUSE
      clipdist 64
  }
  else
    ifactor ROBOTMOUSE
  {
      ifdead
      {
        ifcount 32
        {
          globalsound MOUSEANNOY
