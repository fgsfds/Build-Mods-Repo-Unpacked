	cstator 256
	geta[].hitag mtype
	seta[].hitag 0
enda

action GLASSBREAKING 2 5 1 1 4
useractor notenemy GLASS 8

	ifaction GLASSBREAKING
	{
		geta[].x startx
		geta[].y starty
		
		set x startx
		rand x2 8
		mul x2 sprite[].xrepeat
		ifrnd 128 add x x2 else sub x x2
		
		set y starty
		rand y2 8
		mul y2 sprite[].xrepeat
		ifrnd 128 add y y2 else sub y y2
		
		seta[].x x
		seta[].y y
		
		lotsofglass 4
		seta[].x startx
		seta[].y starty
		ifactioncount 5 killit
	}
	ifn mtype 0
	{
		ifhitweapon
		{
			ifdead
			{
				state glass_sound
				action GLASSBREAKING
			}
		}
	}

