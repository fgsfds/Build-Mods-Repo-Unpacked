defstate horiz_eval

ifvarand sprite[].cstat 32
{
	set temp NO
	findplayer TMP_A
	geta[].z z
	sub z player[].posz
	
	ifl player[].posz sprite[].z mul z -8 else mul z 8
	div z TMP_A
		
	add z 100
	
	ifl player[].posz sprite[].z { ifle player[].horiz z set temp YES }
	else
	ifge player[].horiz z set temp YES
}
else set temp YES

ends

