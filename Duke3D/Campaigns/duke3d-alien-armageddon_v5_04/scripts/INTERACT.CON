spriteshadow BOWLINGPIN
// spriteshadow BOWLINGBALL
eventloadactor 4426 cactor BOWLINGPIN cstator 257 enda
eventloadactor BOWLINGPIN cstator 257 enda

action APINDOWN 1
useractor notenemy BOWLINGPIN 0

	fall
	
	ifaction 0
	{
		clipdist 8
		findnearactorz BOWLINGBALL 384 3072 spriteid
		ifn spriteid -1
		ifn actorvar[spriteid].dodgetime 0
		{
			geta[].x x2
			geta[].y y2
			sub x2 sprite[spriteid].x
			sub y2 sprite[spriteid].y
			getangle angvar x2 y2
			
			
			seta[].mdflags 16
			action APINDOWN
			seta[].ang angvar
			cstat 0
			move STAKEVELS geth
			sound BOWLING_PIN
			getav[spriteid].countvar countvar
			add countvar 1
			setav[spriteid].countvar countvar
			set myvictim spriteid
		}
		else
		ifhitweapon
		{
			seta[].mdflags 16
			action APINDOWN
			geta[].htang angvar
			
			sub angvar 96
			rand temp 192
			add angvar temp
			seta[].ang angvar
			cstat 0
			move STAKEVELS geth
			sound BOWLING_PIN
		}
	}
	strength 0
	ifmove STAKEVELS 
	{
		ifn sprite[].htmovflag 0 
		{
			geta[].htmovflag spriteid
			addvar spriteid 16384
			ifvarl spriteid 16384 ifvarg spriteid -1 
			{
				ife sprite[spriteid].picnum BOWLINGPIN
				{
					seta[spriteid].htextra 2
					
					seta[spriteid].htpicnum KNEE
					seta[spriteid].htowner THISACTOR
					
					geta[spriteid].x x2
					geta[spriteid].y y2
					sub x2 sprite[].x
					sub y2 sprite[].y
					getangle angvar x2 y2
					seta[spriteid].htang angvar
					ifn myvictim -1
					{
						getav[myvictim].countvar countvar
						add countvar 1
						setav[myvictim].countvar countvar
					}
				}
			}
		}
		ifcount 6 move STOPPED geth
	}

enda

eventloadactor BOWLINGBALL sizeat 40 40 enda

useractor notenemy BOWLINGBALL 0

	
	ifmove 0
	{
		move STOPPED
		clipdist 32
		cstator 257
		seta[].mdzoff -1560
		sizeat 40 40
	}
	
	ife dodgetime 0
	state checkheldobject
	
	
	// when rolled
	// seta[].ang player[].ang
	// seta[].htextra 10
	// seta[].htpicnum KNEE
	// seta[].htowner player[].i
	// seta[].htang player[].ang
		
	ifg sprite[].htextra 0
	{
		geta[].htextra temp
		
		mul temp 160
		ife sprite[].htpicnum KNEE mul temp 2
		
		div temp tiledata[sprite[].picnum].xsize
		ifg temp dodgetime
		{
			set angvel sprite[].htang
			set dodgetime temp
			seta[].ang angvel
		}
	}
	ifhitweapon strength 0
	
	ife heldobject THISACTOR { set dodgetime 0 break }
	fall
	
	ifg sprite[].zvel 0 add FEMFALLDMG 1 else
	ifg FEMFALLDMG 0
	{
		set FEMFALLDMG 0
		ifg dodgetime 0 sound BOWLING_ROLL
	}

	ifvarg dodgetime 0
	{
		seta[].ang angvel
		
		geta[].pitch temp
		set tempb dodgetime
		shiftl tempb 1
		add temp tempb
		seta[].pitch temp
		
		
		cos xvel angvel
		sin yvel angvel
		setvarvar temp dodgetime
		ifvarg temp 1024 setvar temp 1024
		mulvarvar xvel temp
		mulvarvar yvel temp
		shiftvarr xvel 13
		shiftvarr yvel 13
		movesprite THISACTOR xvel yvel 0 CLIPMASK0 RETURN 
		// shiftvarr dodgetime 1
		sub dodgetime 1
		ifn RETURN 0 
		{
			setvarvar temp RETURN
			andvar temp 49152
			ifvare temp 32768
			{
				// hit a wall
				setvarvar hitwall RETURN
				andvar hitwall 16383
				ife wall[hitwall].pal 4 { stopactorsound THISACTOR BOWLING_ROLL killit }
				getwall[hitwall].point2 B
				getwall[hitwall].x x2
				getwall[hitwall].y y2
				getwall[B].x x
				getwall[B].y y
				subvarvar x2 x
				subvarvar y2 y
				getangle angvar x2 y2
				
				getactor[THISACTOR].ang tempb
				getincangle tempc angvar tempb
				subvarvar angvar tempc
				ifvarg angvar 2047 subvar angvar 2048
				ifvarl angvar 0 addvar angvar 2048
				setactor[THISACTOR].ang angvar
				set angvel angvar
				div dodgetime 2
			}
/*	
			// cause damage to hit sprite
			addvar RETURN 16384
			ifvarl RETURN 16384 ifvarg RETURN -1 
			{
				seta[RETURN].htextra 3
				seta[RETURN].htpicnum KNEE
				seta[RETURN].htowner THISACTOR
				seta[RETURN].htang angvel
			}
			else
			set dodgetime 0
*/
		}
		ifvarl dodgetime 2 setvar dodgetime 0
		ifvarl dodgetime 10 stopactorsound THISACTOR BOWLING_ROLL
		ifg countvar 4
		{
			set countvar 0
			soundonce BOWLING_STRIKE
			stopsound BOWLING_PIN
		}
	}

enda

